# NER Annotation Tool - Development Environment
NODE_ENV=development
PORT=3001

# Frontend Configuration
VITE_API_URL=http://localhost:3001
VITE_APP_TITLE=NER Annotation Tool
VITE_ENABLE_DEBUG=true

# File Upload Configuration
